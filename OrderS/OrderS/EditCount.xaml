<Window x:Class="OrderS.EditCount"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:OrderS"
        mc:Ignorable="d"
        Title="EditCount" Height="200" Width="300">
    <Grid>
        <Button Name="Submit" Click="Submit_OnClick" BorderThickness="0" Content="Submit" HorizontalAlignment="Left" Margin="50,135,0,0" VerticalAlignment="Top"/>
        <TextBlock Name="Text" HorizontalAlignment="Left" Margin="50,33,0,0" Text="Enter number for product count" TextWrapping="Wrap" VerticalAlignment="Top"/>
        <TextBox Name="Count" HorizontalAlignment="Left" Margin="50,81,0,0" Text="1" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>

    </Grid>
</Window>
